}

.mainBg {
  position: absolute;
  top: 150rpx;
  left: 0;
  width: 100%;
  height: 635rpx;
  background: url(https://inimg02.jiuyan.info/in/2017/11/21/8542AF8C-D4B9-9E35-6454-F389531A8514.png);
