/**index.wxss**/

.container {
  padding-bottom: 150rpx;
}

.c-bottom-nav {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 100rpx;
  width: 100%;
  font-size: 0;
  background-color: #fff;
  box-shadow: rgba(0, 0, 0, 0.1) 0px -2px 10px;
}

.c-bottom-nav .btn-end-act {
  width: 100%;
  height: 100%;
  text-align: center;
  background-color: #B0B0B0;
  color: #fff;
  font-size: 36rpx;
  line-height: 100rpx;
}

.c-bottom-nav .s-btn {
  width: 50%;
  font-size: 36rpx;
  display: inline-block;
  text-align: center;
  color: #333333;
  line-height: 100rpx;
}

.c-bottom-nav .s-btn.btn-sign {
  background-color: #FF4338;
  color: #fff;
}

.c-main-swiper {
  width: 100%;
  height: 500rpx;
  position: relative;
}

.c-main-swiper swiper-item .slide-image {
  width: 100%;
  height: 100%;
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}

.c-main-swiper .IndicatorDots {
  position: absolute;
  bottom: 50rpx;
  right: 40rpx;
  z-index: 10;
}

.c-main-swiper .IndicatorDots .dot {
  width: 12rpx;
  height: 12rpx;
  background-color: #D8D8D8;
  border-radius: 50%;
  display: inline-block;
  margin: 0 5rpx;
}

.c-main-swiper .IndicatorDots .dot.active {
  width: 24rpx;
  border-radius: 12rpx;
  background-color: #fff;
}

.c-other {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
  box-sizing: border-box;
  padding: 20rpx;
  border-top: solid 1rpx #E0E0E0;
  box-shadow: rgba(0, 0, 0, 0.1) 0px -2px 10px;
  background-image: -webkit-linear-gradient(-180deg, #fff, #f2f2f2);
}

.c-other view {
  display: inline-block;
  vertical-align: middle;
}

.c-other .s-logo {
  width: 64rpx;
  height: 64rpx;
  background-image: url(https://inimg01.jiuyan.info/in/2017/10/15/6CD1BC72-FBBB-BC4E-528E-50943934E20F.jpg);
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center;
  margin-right: 16rpx;
}

.c-other .s-text {
  color: #333333;
  font-size: 28rpx;
}

.c-other .s-look {
  width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #FF4338;
  font-size: 28rpx;
  border: solid 1rpx #FF4338;
  text-align: center;
  border-radius: 8rpx;
  float: right;
}

.c-headLine {
  position: absolute;
  bottom: 0;
  left: 0;
  box-sizing: border-box;
  padding: 60rpx 40rpx 40rpx 40rpx;
  color: #fff;
  width: 100%;
  background-image: -webkit-linear-gradient(270deg, rgba(0, 0, 0, 0), #333);
}

.c-headLine .s-title {
  font-size: 40rpx;
  margin-bottom: 25rpx
}

.c-headLine .s-desc {
  font-size: 22rpx
}

.c-infos {
  width: 100%;
  box-sizing: border-box;
  padding: 0 40rpx;
}

.c-infos .s-line {
  border-bottom: 1rpx solid #E0E0E0;
  padding: 50rpx 0;
  color: #666666;
  font-size: 28rpx;
}

.c-infos .s-header {
  color: #333333;
}

.c-infos .s-header view {
  padding-left: 30rpx;
  background-repeat: no-repeat;
  background-size: 24rpx auto;
  background-position: left center;
}

.c-infos .s-header .s-addr {
  margin-bottom: 28rpx;
  background-image: url(https://inimg01.jiuyan.info/in/2017/10/13/07690A0B-A917-5A50-D8FC-AB582E3413D1.jpg)
}

.c-infos .s-header .s-time {
  background-image: url(https://inimg01.jiuyan.info/in/2017/10/13/39306798-92E8-A50F-72AF-60850E71041E.jpg)
}

.c-infos .s-line:last-child {
  border: none;
}

.c-infos .s-line .title {
  color: #333333;
  font-size: 34rpx;
  margin-bottom: 15rpx;
}

.c-infos .mm-user {
  padding: 40rpx 0;
  font-size: 0;
  border-bottom: 1rpx solid #E0E0E0;
}

.c-infos .m-more {
  display: inline-block;
  color: #43DDF5;
  font-size: 28rpx;
}

.c-infos .m-intro {
  display: -webkit-box;
}

.mm-user:last-child {
  border: none
}

.mm-user .user-avatar {
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
  display: inline-block;
  vertical-align: middle;
  border-radius: 50%;
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
  position: relative;
}

.mm-user .user-avatar .sex {
  position: absolute;
  bottom: 0;
  right: 0;
  width: 24rpx;
  height: 24rpx;
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
}

.mm-user .user-avatar .sex.male {
  background-image: url(https://inimg01.jiuyan.info/in/2017/10/13/AC7CA472-C19B-62EF-952B-97CAAD1C65F0.jpg)
}

.mm-user .user-avatar .sex.female {
  background-image: url(https://inimg01.jiuyan.info/in/2017/10/13/E72C5ACB-CCD1-7428-FD66-2AD0662288CF.jpg)
}

.mm-user .user-info {
  display: inline-block;
  vertical-align: middle
}

.mm-user .user-nick {
  font-size: 28rpx;
  color: #333333
}

.mm-user .user-desc {
  font-size: 22rpx;
  color: #B0B0B0
}

.c-invite-wrap .s-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 0 auto;
  border-radius: 50%;
  background-image: url(https://inimg01.jiuyan.info/in/2017/10/15/6CD1BC72-FBBB-BC4E-528E-50943934E20F.jpg);
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center;
}

.c-invite-wrap .s-desc {
  color: #B0B0B0;
  font-size: 24rpx;
  text-align: center;
  margin-top: 20rpx;
}

.c-invite-wrap .s-title {
  color: #333333;
  font-size: 36rpx;
  text-align: center;
  margin: 20rpx auto 0;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 360rpx;
}

.c-invite-wrap .s-qr {
  width: 200rpx;
  height: 200rpx;
  margin: 40rpx auto;
  border-radius: 50%;
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center;
}

.c-invite-wrap .s-btn {
  width: 460rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  text-align: center;
  margin: 0 auto;
  border-radius: 200rpx;
}

.c-invite-wrap .s-transmit {
  color: #fff;
  background-color: #FF4338;
  position: relative;
}

.c-invite-wrap .s-transmit button {
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.c-invite-wrap .s-share-img {
  background-color: #fff;
  color: #333333;
  border: solid 1rpx #E0E0E0;
  margin-top: 20px;
}

.c-modal .c-book-wrap {
  padding: 50rpx 0;
  padding-top: 43prx;
  padding-bottom: 0px;
  position: absolute;
  top: 50%;
  left: 95rpx;
  transform: translate(0, -50%);
  margin: 0 auto;
}

.c-book-wrap .s-bottom-text {
  color: #454545;
  font-size: 28rpx;
}

.s-bottom-btn {
  position: relative;
}

.c-modal .c-book-wrap contact-button {
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
  width: 100%;
  height: 100%;
}

.c-modal .c-book-wrap button {
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
  width: 100%;
  height: 100%;
}

.c-book-wrap .s-qun {
  width: 420rpx;
  height: 500rpx;
  margin: 55rpx auto;
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center;
}